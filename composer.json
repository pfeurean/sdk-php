{
    "name": "pfeurean/authorizenet",
    "type": "library",
    "description": "Official PHP SDK for Authorize.Net (with a BluePay Patch)",
    "keywords": ["authorizenet", "authorize.net", "payment", "ecommerce", "bluepay"],
    "license": "proprietary",
    "homepage": "http://github.com/pfeurean",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "goetas-webservices/xsd2php-runtime":"^0.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpmd/phpmd": "~2.0"
    },
    "autoload": {
        "classmap": ["lib"]
    },
    "autoload-dev": {
        "classmap": ["tests"]
    }   
}
